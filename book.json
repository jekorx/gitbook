{
  "title": "jekorx's blog",
  "description": "个人学习、工作经验的总结",
  "author": "jekorx",
  "language": "zh-hans",
  "plugins": [
    "-lunr",
    "-search",
    "-sharing",
    "-highlight",
    "prism",
    "github",
    "search-pro",
    "sharing-plus",
    "tbfed-pagefooter",
    "back-to-top-button",
    "expandable-chapters-small",
    "page-treeview"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/jekorx"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "page-treeview": {
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    },
    "tbfed-pagefooter": {
      "copyright": "© 2023 www.wdg.pub",
      "modify_label": "Last modified: ",
      "modify_format": "YYYY-MM-DD"
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "linkedin": false,
      "qq": false,
      "qzone": false,
      "twitter": false,
      "weibo": false,
      "whatsapp": false,
      "all": [
        "douban",
        "facebook",
        "google",
        "linkedin",
        "qq",
        "qzone",
        "twitter",
        "weibo",
        "whatsapp"
      ]
    }
  }
}